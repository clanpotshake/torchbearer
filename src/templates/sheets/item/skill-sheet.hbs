<form class="{{cssClass}}" autocomplete="off">
  {{#> systems/torchbearer/templates/sheets/item/components/sheet-header.hbs}}
  {{/systems/torchbearer/templates/sheets/item/components/sheet-header.hbs}}

  {{!-- Common Item body --}}
  <div class="side-property">
    <label for="data.luck">{{localize "TB2.SkillBeginnersLuckLabel"}}</label>
    <select name="data.luck" data-type="String">
      {{#select data.data.luck}}
        {{#each (beginnersLuckAttributes) as |value key|}}
          <option value="{{value}}">{{localize (lookup ../config.i18n.attributes value)}}</option>
        {{/each}}
      {{/select}}
    </select>
  </div>
  {{#> systems/torchbearer/templates/sheets/item/components/body.hbs}}
  {{/systems/torchbearer/templates/sheets/item/components/body.hbs}}
</form>
